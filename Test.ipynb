   ],
   "source": [
    "thresh = cv2.adaptiveThreshold(gray, 255, cv2.ADAPTIVE_THRESH_MEAN_C, cv2.THRESH_BINARY,11,2);\n",
    "thresh2 = cv2.threshold(cv2.GaussianBlur(gray,(3,3),0), 0, 255, cv2.THRESH_BINARY | cv2.THRESH_OTSU)[1]\n",
    "\n",
    "\"\"\"cv2.imshow(\"Thresh\",thresh)\n",
    "cv2.imshow(\"Thresh2\",thresh2)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\"\"\""
   ]
  },
  {
