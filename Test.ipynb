     "metadata": {},
     "execution_count": 8
    }
   ],
   "source": [
    "rectKern = cv2.getStructuringElement(cv2.MORPH_RECT, (13, 5))\n",
    "tophat= cv2.morphologyEx(gray, cv2.MORPH_TOPHAT, rectKern)\n",
    "tophat2= cv2.morphologyEx(cv2.GaussianBlur(gray,(5,5),0), cv2.MORPH_TOPHAT, rectKern)\n",
    "\n",
    "\"\"\"cv2.imshow(\"Tophat\",tophat)\n",
    "cv2.imshow(\"Tophat2\",tophat2)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()\"\"\""
   ]
  },
  {
   "cell_type": "code",
