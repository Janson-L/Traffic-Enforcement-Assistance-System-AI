   "outputs": [],
   "source": [
    "threshA = cv2.bitwise_and(threshXBED, threshXBED, mask=lightMT)\n",
    "threshA = cv2.dilate(threshA, None, iterations=2)\n",
    "threshA = cv2.erode(threshA, None, iterations=1)\n",
    "cv2.imshow(\"Grad Erode Dilate\", threshXBED)\t\n",
